<mvc:View 
    controllerName="com.prestativ.slc.followup.controller.App"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m"
    xmlns:f="sap.f"
>
    
    <App 
        id="app"
        busy="{appView>/busy}"
        busyIndicatorDelay="{appView>/busyIndicatorDelay}"
    >
        <f:FlexibleColumnLayout
            id="layout"
            layout="{appView>/layout}"
            backgroundDesign="translucent"
        >

        </f:FlexibleColumnLayout>
    </App>
</mvc:View>
